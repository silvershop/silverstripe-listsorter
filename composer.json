{
    "name": "silvershop/silverstripe-listsorter",
    "description": "Easily provide front-end sorting controls for SilverStripe lists",
    "type": "silverstripe-vendormodule",
    "keywords": ["sort","datalist","arraylist","list","sortable"],
    "replace": {
        "burnbright/silverstripe-listsorter": "2.*"
    },
    "authors": [
        {
            "name": "Jeremy Shipman",
            "email": "jeremy@burnbright.net"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0 | ^5.0"
    },
    "extra": {
         "branch-alias": {
             "dev-master": "3.x-dev"
         }
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5",
      "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverShop\\ListSorter\\": "src/",
            "SilverShop\\ListSorter\\Tests\\": "tests/php/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "license": "BSD-3-Clause"
}
